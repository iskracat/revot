#: templates/ballot_message.txt:2
#, python-format
msgid ""
"Hello %(name)s,\n"
"\n"
"You have been enrolled in the voting identified as «%(id)s».\n"
"\n"
"The polling station will remain open from %(s)s to %(e)s (Manresa time). "
"To cast your ballot please follow the following link while polling "
"station is open."
msgstr ""
"Hola %(name)s,\n"
"\n"
"Formeu part del cens electoral de la votació identificada com «%(id)s».\n"
"\n"
"La mesa electoral esdevindrà oberta de %(s)s fins %(e)s (hora de "
"Manresa). Per votar seguiu el següent enllaç mentre la mesa sigui oberta."

#: templates/ballot_message.txt:12
msgid "For you interest, we show below the ballot corresponding to this voting."
msgstr ""
"Pel vostre interès, seguidament us mostrem la papereta que correspon a "
"aquesta votació."

#: templates/ballot_message.txt:16
#, python-format
msgid "You will be commited to choose one option.\n"
msgid_plural ""
"\n"
"You will be commited to choose a maximum of %(m)s options."
msgstr[0] "Esteu convidat a triar una opció.\n"
msgstr[1] ""
"\n"
"Esteu convidat a triar un màxim de %(m)s opcions."

